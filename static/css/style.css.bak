/*common css  */

*,*::before,*::after{
	padding:0;
	margin:0;
	box-sizing:border-box;
}

html{
	scroll-behavior: smooth;
}

body{
	font-size:1rem;
}
h1,h2,h3,h4,h5,h5,h6,ul,p,ul{
	margin:0;
}

ul{
	list-style:none;
}

a,a:hover{
	text-decoration:none;
}

img{
	max-width:100%;
	width:100%;
}

:root {
  --main-color: hotpink;
  --pane-padding: 5px 42px;
}

.pt-20{
	padding-top:20px;
}

.pb-20{
	padding-bottom:20px;
}

